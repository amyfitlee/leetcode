class Solution {
public:
    int findMin(vector<int>& nums) {
        int l = 0, r = nums.size() - 1, mid = 0;
        while(l < r){
            if(nums[l] < nums[r]) return nums[l];
            mid = (l + r) / 2;
            if(nums[mid] >= nums[l]) l = mid + 1;
            else r = mid;
        }
        return nums[l];
        
    }
};


class Solution {
public:
    int findMin(vector<int>& nums) {
        int l = 0, r = nums.size() - 1, mid = 0;
        while(l < r){
            mid = l + (r - l) / 2;
            if(nums[mid] > nums[r]){
                l = mid + 1;
            }
            else{
                r = mid;
            }
        }
        return nums[l];
    }
};
