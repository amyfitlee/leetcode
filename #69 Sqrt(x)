class Solution {
public:
	int mySqrt(int x) {
        if(x <= 1) return x;
        int l = 0, r = x, mid = 0;
        while(l < r){
            mid = (l + r) / 2;
            if(mid <= x / mid && (mid + 1) > x / (mid + 1)) return mid;
            if(mid < x / mid) l = mid + 1;
            else r = mid - 1;
        }
        return l;
	}
};  
